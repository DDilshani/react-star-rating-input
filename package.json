{
  "name": "react-star-rating-input",
  "version": "5.0.0",
  "description": "React.js component for entering 0-5 stars",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint tests src index.js demo.js",
    "test": "mocha --require ./test-hook -b tests/*.spec.js",
    "build": "browserify demo.js | uglifyjs > www/bundle.js"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ikr/react-star-rating-input.git"
  },
  "keywords": [
    "reactjs",
    "component",
    "react-component",
    "html",
    "form",
    "browser",
    "browserify",
    "input",
    "control",
    "widget",
    "stars",
    "rating"
  ],
  "author": "Ivan Krechetov <ikr@ikr.su>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikr/react-star-rating-input/issues"
  },
  "dependencies": {
    "react": "^0.12.0",
    "react-intl": "^1.2.0"
  },
  "devDependencies": {
    "brfs": "^1.2.0",
    "browserify": "^6.3.2",
    "insert-css": "^0.2.0",
    "intl": "^0.1.4",
    "jsdom": "^1.2.1",
    "jsdom-test-browser": "^2.0.0",
    "jshint": "^2.5.5",
    "mocha": "^1.21.4",
    "sinon": "^1.10.3",
    "uglify-js": "^2.4.15"
  }
}
